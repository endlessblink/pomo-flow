{
  "name": "skill-router",
  "version": "1.0.0",
  "description": "Automatic Skill Discovery and Integration System for Claude Code",
  "main": "skill-discovery.js",
  "bin": {
    "skill-router": "cli-tool.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node cli-tool.js",
    "test": "node cli-tool.js discover",
    "watch": "node cli-tool.js watch --auto-integrate",
    "status": "node cli-tool.js status",
    "integrate": "node cli-tool.js integrate",
    "analyze": "node cli-tool.js analyze",
    "backup": "node cli-tool.js backup --list",
    "install-global": "npm link"
  },
  "keywords": [
    "claude-code",
    "skill-router",
    "automation",
    "discovery",
    "integration",
    "learning",
    "patterns"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "lodash": "^4.17.21",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/lodash": "^4.14.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/skill-router.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/skill-router/issues"
  },
  "homepage": "https://github.com/your-repo/skill-router#readme"
}