{
  "name": "pomo-flow",
  "version": "1.0.0",
  "description": "Multi-view productivity app with integrated task management and pomodoro timer - Vue 3 Edition",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5546",
    "kill": "bash kill-pomo.sh",
    "build": "npx vite build --mode production",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:watch": "vitest --ui",
    "test:safety": "vitest run tests/safety/",
    "watch:ts": "nodemon --watch src --ext ts,vue --exec \"npx tsc --noEmit --skipLibCheck\"",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "validate:imports": "node scripts/validate-imports.js",
    "validate:css": "node scripts/validate-css.js",
    "validate:dependencies": "vitest run tests/safety/dependencies.test.ts",
    "validate:all": "npm run validate:imports && npm run validate:css && npm run validate:dependencies && npm run lint",
    "validate:root": "bash scripts/validate-root-structure.sh",
    "validate:file-organization": "node scripts/file-organizer.cjs stats",
    "validate:comprehensive": "npm run validate:root && npm run validate:file-organization && npm run validate:all",
    "file:organize": "node scripts/file-organizer.cjs organize",
    "file:organize:all": "node scripts/file-organizer.cjs organize . && node scripts/file-organizer.cjs organize scripts && node scripts/file-organizer.cjs organize .claude",
    "file:watch": "node scripts/file-organizer.cjs watch",
    "file:stats": "node scripts/file-organizer.cjs stats",
    "file:cleanup": "bash scripts/cleanup-system-files.sh && npm run file:organize",
    "file:install-hooks": "bash scripts/install-git-hooks.sh",
    "file:uninstall-hooks": "rm -f .git/hooks/pre-commit .git/hooks/pre-commit.backup",
    "generate-tokens": "node scripts/generate-tokens.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "skills:telemetry-report": "node scripts/skills-telemetry.cjs report",
    "skills:telemetry-export": "node scripts/skills-telemetry.cjs export",
    "skills:telemetry-reset": "node scripts/skills-telemetry.cjs reset",
    "skills:telemetry-status": "node scripts/skills-telemetry.cjs status",
    "skills:telemetry-analyze": "node scripts/skills-telemetry.cjs analyze",
    "skills:init-instrumentation": "echo 'âœ… Skill telemetry infrastructure initialized' && echo 'ðŸ“Š Data will be collected to: docs/skill-telemetry/usage-events.jsonl' && echo 'ðŸ”§ Set SKILLS_TELEMETRY_ENABLED=false to disable collection'",
    "skills:consolidation-analyze": "node scripts/skill-consolidation-analyzer.cjs",
    "skills:consolidation-execute": "node scripts/skill-consolidation-executor.cjs",
    "telemetry:health": "node scripts/verify-telemetry-system.cjs",
    "telemetry:watch": "node scripts/telemetry-watch.cjs",
    "telemetry:daily": "node scripts/daily-telemetry-summary.cjs",
    "consolidation:check-progress": "node scripts/phase-progress-checker.cjs",
    "consolidation:sync-tasks": "node scripts/sync-consolidation-to-markdown.cjs",
    "catalog:generate": "node scripts/update-skill-catalog.cjs",
    "catalog:update": "node scripts/update-skill-catalog.cjs",
    "catalog:docs": "node scripts/build-documentation-graph.cjs",
    "skill-logs": "node .claude/skill-logger.js",
    "skill-logs:recent": "node .claude/skill-logger.js recent",
    "skill-logs:metrics": "node .claude/skill-logger.js metrics",
    "skill-logs:analyze": "node .claude/utils/log-analyzer.js patterns",
    "skill-logs:performance": "node .claude/utils/log-analyzer.js performance",
    "skill-logs:export": "node .claude/utils/log-analyzer.js export json 7",
    "skill-logs:cleanup": "node .claude/skill-logger.js cleanup",
    "skill-logs:server": "node .claude/mcp-logging-server/server.js",
    "skill-logs:start": "node start-skill-monitoring-simple.js",
    "skill-logs:dashboard": "node start-dashboard-server.js",
    "skill-logs:monitor": "node simple-skill-monitor.js",
    "skill-monitor": "node simple-skill-monitor.js",
    "skill-dashboard": "node simple-skill-monitor.js",
    "skill-monitor:full": "node start-skill-monitoring-simple.js",
    "skills:dashboard": "cd .claude/skills-management && npm run dev -- --port 6008",
    "backup-tasks": "node scripts/backup-tasks.cjs",
    "restore-tasks": "node scripts/restore-tasks.cjs",
    "checkpoint": "bash scripts/checkpoint-with-backup.sh",
    "backups:list": "node scripts/restore-tasks.cjs --list",
    "backups:validate": "node scripts/restore-tasks.cjs",
    "clean:preview": "node scripts/clean-root.js --dry-run",
    "clean:tests": "node scripts/clean-root.js --type=tests",
    "clean:builds": "node scripts/clean-root.js --type=builds",
    "clean:cache": "node scripts/clean-root.js --type=cache",
    "clean:dev": "node scripts/clean-root.js --type=dev",
    "clean:all": "node scripts/clean-root.js --type=all",
    "clean:organize": "node scripts/comprehensive-root-cleanup.js",
    "test:user-flows": "node .claude/skills/user-flow-testing-skills/run-skill.mjs",
    "test:task-flows": "npx playwright test tests/user-flows/task-management.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:timer-flows": "npx playwright test tests/user-flows/timer-workflows.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:navigation-flows": "npx playwright test tests/user-flows/navigation.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:canvas-flows": "npx playwright test tests/user-flows/canvas-interactions.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:calendar-flows": "npx playwright test tests/user-flows/calendar-operations.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:advanced-flows": "npx playwright test tests/user-flows/advanced-features.spec.js --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs",
    "test:user-flows:debug": "npx playwright test tests/user-flows/ --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs --debug",
    "test:user-flows:ui": "npx playwright test tests/user-flows/ --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs --ui",
    "test:user-flows:report": "npx playwright test tests/user-flows/ --config=.claude/skills/user-flow-testing-skills/config/playwright.config.cjs --reporter=html",
    "tm:status": "cat task-management/00-START.md",
    "kanban": "open task-management/kanban-ui.html",
    "tm:ui": "open task-management/kanban-ui.html",
    "tm:kanban": "vim task-management/kanban.md",
    "tm:ideas": "vim task-management/ideas.md",
    "tm:roadmap": "vim task-management/planning/ROADMAP.md",
    "tm:arch": "vim task-management/planning/ARCHITECTURE.md",
    "tm:archive": "vim task-management/archive.md",
    "mcp:setup": "bash scripts/check-wsl2-swap.sh && node scripts/validate-mcp-startup.cjs",
    "mcp:cleanup": "bash scripts/cleanup-mcp-zombies.sh",
    "mcp:check-health": "bash scripts/check-wsl2-swap.sh",
    "mcp:monitor": "node src/mcp-health-monitor.cjs",
    "mcp:monitor:watch": "nodemon --watch src src/mcp-health-monitor.cjs",
    "mcp:crash-monitor": "node src/mcp-crash-monitor-fixed.cjs start",
    "mcp:crash-check": "node src/mcp-crash-monitor-fixed.cjs check",
    "mcp:crash-report": "node src/mcp-crash-monitor-fixed.cjs report",
    "mcp:crash-analysis": "node scripts/analyze-mcp-crashes.cjs",
    "mcp:restart": "bash scripts/cleanup-mcp-zombies.sh && bash scripts/check-wsl2-swap.sh",
    "node-version": "node --version",
    "check-engines": "node --version && npm --version",
    "type-check:watch": "concurrently -n \"VUE,TSC\" -c \"magenta,blue\" --kill-others-on-fail \"vue-tsc --noEmit --watch\" \"tsc --noEmit --watch --pretty\"",
    "type-check:track": "chokidar 'src/**/*.ts' 'src/**/*.vue' --command 'tsc --noEmit --pretty > .typescript-errors.log 2>&1 || true' --initial --silent",
    "conflict:analyze": "node scripts/analyze-conflicts.cjs",
    "conflict:baseline": "node scripts/analyze-conflicts.cjs > docs/conflict-systems-resolution/baseline-$(date +%Y%m%d).txt",
    "conflict:hooks": "bash scripts/install-git-hooks.sh",
    "test:conflict-system": "vitest run --testNamePattern='conflict|Conflict'"
  },
  "keywords": [
    "productivity",
    "pomodoro",
    "task-management",
    "canvas",
    "multi-view",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/pouchdb": "^6.4.2",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "@vue-flow/node-resizer": "^1.5.0",
    "@vueuse/core": "^10.11.1",
    "@vueuse/gesture": "^2.0.0",
    "chokidar": "^4.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "lucide-vue-next": "^0.294.0",
    "naive-ui": "^2.43.1",
    "pinia": "^2.1.7",
    "pouchdb": "^7.2.2",
    "pouchdb-browser": "^9.0.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.0",
    "vue-cal": "^4.10.2",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.8.1",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "nodemon": "^3.1.11",
    "postcss": "^8.4.32",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.0",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.5"
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  }
}
